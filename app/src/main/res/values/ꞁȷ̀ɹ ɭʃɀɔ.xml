<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Iitxe_Chelesai" parent="@style/Widget.MaterialComponents.CardView">
        <item name="shapeAppearanceOverlay">@style/ShapeAppearanceOverlay_card_corners</item>
    </style>

    <style name="ShapeAppearanceOverlay_card_corners">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeTopRight">@dimen/paaksiicamii</item>
        <item name="cornerSizeTopLeft">@dimen/paaksiica</item>
        <item name="cornerSizeBottomRight">@dimen/paaksiica</item>
        <item name="cornerSizeBottomLeft">@dimen/paaksiicamii</item>
        <item name="elevation">0dp</item>
        <item name="cardBackgroundColor">@color/Piisa</item>
        <item name="strokeColor">@color/Tiisha</item>
        <item name="strokeWidth">0.25dp</item>
    </style>

    <style name="Iitxe_Ciihii" parent="@style/Widget.MaterialComponents.CardView">
        <item name="shapeAppearanceOverlay">@style/ShapeAppearanceOverlay_card_inner_corners</item>
    </style>

    <style name="ShapeAppearanceOverlay_card_inner_corners">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeTopRight">@dimen/ciihiimii</item>
        <item name="cornerSizeTopLeft">@dimen/ciihii</item>
        <item name="cornerSizeBottomRight">@dimen/ciihii</item>
        <item name="cornerSizeBottomLeft">@dimen/ciihiimii</item>
        <item name="elevation">0dp</item>
        <item name="cardBackgroundColor">@color/Tiisha</item>
        <item name="strokeColor">@color/Tawa</item>
        <item name="strokeWidth">0.25dp</item>
    </style>


    <style name="Iitxe_Cazelesh" parent="@style/Widget.MaterialComponents.CardView">
        <item name="shapeAppearanceOverlay">@style/ShapeAppearanceOverlay_button_corners</item>
    </style>

    <style name="ShapeAppearanceOverlay_button_corners">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeTopRight">@dimen/catsiinamii</item>
        <item name="cornerSizeTopLeft">@dimen/catsiina</item>
        <item name="cornerSizeBottomRight">@dimen/catsiina</item>
        <item name="cornerSizeBottomLeft">@dimen/catsiinamii</item>
        <item name="android:shadowColor">@android:color/transparent</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:elevation">0dp</item>
        <item name="android:padding">8dp</item>
        <item name="cardBackgroundColor">@color/Tiisha</item>
        <item name="strokeColor">@color/Tawa</item>
        <item name="strokeWidth">0.25dp</item>
    </style>

    <style name="Iitxe_Cazelesh_Ciihii" parent="@style/Widget.MaterialComponents.CardView">
        <item name="shapeAppearanceOverlay">@style/ShapeAppearanceOverlay_button_corners2</item>
    </style>

    <style name="ShapeAppearanceOverlay_button_corners2">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeTopRight">@dimen/catsiinaciihiimii</item>
        <item name="cornerSizeTopLeft">@dimen/catsiinaciihii</item>
        <item name="cornerSizeBottomRight">@dimen/catsiinaciihii</item>
        <item name="cornerSizeBottomLeft">@dimen/catsiinaciihiimii</item>
        <item name="android:shadowColor">@android:color/transparent</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:elevation">0dp</item>
        <item name="android:padding">8dp</item>
        <item name="cardBackgroundColor">@color/Tawa</item>
        <item name="strokeColor">@color/Paarha</item>
        <item name="strokeWidth">0.25dp</item>
    </style>


    <style name="LokefiiniiKantoni">
        <item name="android:scrollbarAlwaysDrawVerticalTrack">true</item>
        <item name="android:scrollbarStyle">insideOverlay</item>
        <item name="android:scrollbars">vertical</item>
        <item name="android:fadeScrollbars">true</item>
        <item name="android:scrollbarThumbVertical">@drawable/sak_kantoni</item>
        <item name="android:scrollbarSize">4dp</item>
        <item name="cornerRadius">24dp</item>
    </style>


    <style name="LokefiiniiIshonwa">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">@drawable/sak_ksaka</item>
    </style>


    <style name="Loofyau">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeTopRight">@dimen/paaksiicamii</item>
        <item name="cornerSizeTopLeft">@dimen/paaksiica</item>
        <item name="cornerSizeBottomRight">@dimen/paaksiica</item>
        <item name="cornerSizeBottomLeft">@dimen/paaksiicamii</item>
    </style>


    <style name="LoofyauMii">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeTopRight">@dimen/catsiina</item>
        <item name="cornerSizeTopLeft">@dimen/catsiinamii</item>
        <item name="cornerSizeBottomRight">@dimen/catsiinamii</item>
        <item name="cornerSizeBottomLeft">@dimen/catsiina</item>
    </style>


    <style name="Iitxe_Neshnoni" parent="@style/Widget.MaterialComponents.CardView">
        <item name="shapeAppearanceOverlay">@style/ShapeAppearanceOverlay_card_inner_corners3</item>
    </style>

    <style name="ShapeAppearanceOverlay_card_inner_corners3">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeTopRight">@dimen/paaksiicamii</item>
        <item name="cornerSizeTopLeft">@dimen/paaksiica</item>
        <item name="cornerSizeBottomRight">@dimen/paaksiica</item>
        <item name="cornerSizeBottomLeft">@dimen/paaksiicamii</item>
        <item name="elevation">0dp</item>
    </style>


    <style name="kef">
        <item name="android:textSize">16sp</item>
    </style>

</resources>